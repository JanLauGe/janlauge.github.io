---
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
 <channel>
  <title>{{ site.title  | xml_escape }} - R</title>
  <description>Posts tagged as 'R'</description>
     <atom:link href="{{ site.url }}/feeds/feed.R.xml" rel="self" type="application/rss+xml" />
  <link>{{ site.url }}/tag/R/</link>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <author>
   <name>{{ Laurens Geffert }}</name>
   <email>{{ laurensgeffert@gmail.com }}</email>
  </author>

  {% for post in site.tags.blog limit:10 %}
   <item>
    <title>{{ post.title | xml_escape }}</title>
    <description>{{ post.content | xml_escape }}</description>
    <link>{{ site.url }}{{ post.url }} </link>
    <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
    <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
   </item>
  {% endfor %}
 </channel>
</rss>
